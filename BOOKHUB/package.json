    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.10.1",
    "react-loader-spinner": "5.3.4",
    "react-rating-stars-component": "2.2.0",
    "react-router-dom": "6.14.0",
    "react-scripts": "5.0.1",
    "react-select": "5.7.3",
